{
  "$schema": "../../node_modules/ng-packagr/ng-package.schema.json",
  "dest": "../../dist/scllib",
  "assets": [
    "src/lib",
    {
      "glob": "**/*",
      "input": "src/lib",
      "output": "esm2020/lib"
    },
    {
      "glob": "**/*",
      "input": "src/lib",
      "output": "fesm2020/lib"
    },
    {
      "glob": "**/*",
      "input": "src/lib",
      "output": "fesm2015/lib"
    }
  ],
  "lib": {
    "entryFile": "src/public-api.ts"
  }
}